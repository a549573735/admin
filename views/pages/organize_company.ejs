            <div class="form-horizontal clearfix" >
                <div class="row clearfix">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <v-select ></v-select>
                        
                            <label class="col-md-1 o-pd-r v-label text-center" for="val-skill">企业：</label>
                            <div class="col-md-2 o-pd">
                               <input type="text" class="form-control" name="company" >
                            </div>
                          
                            <div class="col-md-12 o-pd o-m-t">
                                    <label class="col-md-1 o-pd-r v-label" for="val-skill">供应商：</label>
                                    <div class="col-md-2 o-pd">
                                       <input type="text" class="form-control" name="provider">
