
<% include ../common/header.ejs %>

<div class="c-content o-pad" id="c-content">
    <div class="clearfix">
       <% include ../common/leftNav.ejs %>
         <div class="c-r" id="admin-company">
            <div class="form-horizontal clearfix" >
                <div class="row clearfix">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <v-select :role="<%=JSON.stringify({type:'DISTRICT'})%>"></v-select>
                            <label class="col-md-1 o-pd-r v-label-s text-center" for="val-skill">企业：</label>
                            <div class="col-md-2 o-pd">
                               <input type="text" class="form-control" name="company" >
                            </div>
                      
                            <div class="v-t-b o-m-p" >
                                <button class="btn btn-md btn-primary btn-block text-center" @click="searchData()">查询</button>
                            </div>
                             <div class="form-group">
                              <div class="col-md-2 col-md-offset-2" style="margin-top:10px;">
                               <div class="v-t-b o-m-p" style='margin-left:0;'>
                                <button class="btn btn-md btn-primary btn-block text-center add_admin_userBtn" data-target="#modal-addCompany" data-toggle="modal" >新增企业</button>
                                </div>
                              </div>
                         </div>
                        </div>
                    </div>
                </div>
            </div>
                
                
            <div class="row v-table" >
                <div class="col-md-12">
                    <div class="block" >
                        <input class="data_tabls" type="hidden" value="<%=JSON.stringify(data)%>">
                        <handle-table-list :datalist="tablsData" ></handle-table-list>
                    </div>
                </div>
                 <v-pages :pagelist="tablsData.page||1"> </v-pages>
        
            </div>

            <!--   新增园区  弹框 －－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－－-->




<div class="modal fade " id="modal-addCompany" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog  ">
        <div class="modal-content">
            <div class="modal-header o-nobor">
                <button type="button" class="close v-tabs-close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body form_Party">

                       <div class="block-content block-content-narrow">

                                    <div class="js-validation-bootstrap form-horizontal" >
                                        <v-select-admin :market="marketId" :park="parkId" :selectsubset="selectsubset"></v-select-admin>                                        
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="val-username">企业名称: <span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                <input class="form-control admin-name" type="text" name="admin-company" >
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="val-username">管理员用户名
: <span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                <input class="form-control admin-username" placeholder="作为登陆账号使用"  type="text" name="admin-username" >
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label" for="val-email">邮箱 <span class="text-danger">*</span></label>
                                            <div class="col-md-7">
                                                <input class="form-control admin-email" placeholder="用来接收密码和通知" type="text" name="val-email" >
                                            </div>
                                        </div>

                                        <input type="hidden" class="admin-parentId" name="parentId">
                                        <input type="hidden" class="admin-id" name="id">
                                        <input type="hidden" class="admin-type" name="type">
                                        <input type="hidden" class="admin-admin" name="admin">
                                        <input type="hidden" class="admin-marketId" name="marketId">
                                        <input type="hidden" class="admin-parkId" name="parkId">

                                       
                                        <div class="form-group">
                                            <div class="col-md-3 col-md-offset-3">
                                                <button class="btn btn-sm btn-primary v-btn-w admin-add-company" @click="createCompany($event)" >提交</button>
                                            </div>
                                            <div class="col-md-6 admin-msg">
                                            </div>
                                        </div>
                                    </div>
                                </div>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal  新增园区  弹框  -->






        </div>
    </div>
</div>
